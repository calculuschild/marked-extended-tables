    "@semantic-release/release-notes-generator": "^14.0.2",
    "babel-jest": "^29.5.0",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.6.0",
    "jest-cli": "^29.7.0",
