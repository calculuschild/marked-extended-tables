{
  "name": "marked-extended-tables",
  "version": "1.1.0",
  "description": "extended Markdown tables for Marked.js",
  "main": "./lib/index.cjs",
  "module": "./src/index.js",
  "browser": "./lib/index.umd.js",
  "type": "module",
  "keywords": [
    "marked",
    "extension",
    "table",
    "span",
    "column",
    "row",
    "header"
